{% macro printIcon(iconID) %}
  <span class="glyphicon glyphicon-{{iconID}}"></span>
{% endmacro %}

{% macro imagePlaceholder(width, height, text = 'Image') %}
<img src="http://placeholder.pics/svg/{{width}}x{{height}}/717171/FFFFFF/{{text}}({{width}}x{{height}})" class='img-responsive' alt="{{text}}" />
{% endmacro %}
