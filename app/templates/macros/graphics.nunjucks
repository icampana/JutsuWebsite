{% macro printIcon(iconID) %}
  <span class="glyphicon glyphicon-{{iconID}}"></span>
{% endmacro %}

{% macro imagePlaceholder(width, height, text = 'Image', class='img-responsive') %}
<img src="http://placeholder.pics/svg/{{width}}x{{height}}/717171/FFFFFF/{{text}}({{width}}x{{height}})" class='{{class}}' alt="{{text}}" />
{% endmacro %}
